{
  "manifest_version": 3,
  "name": "Track my application",
  "version": "1.0.0",
  "description": "Track My Application allows users to update the status of each job they've applied to.",
  "key": "",
  "oauth2": {
    "client_id": "847763334252-uuug0s9d4mm6ngumg07jsnsm2p0md6cs.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "action": {
    "default_popup": "track.html",
    "default_title": "FriendBlock, friends face's in a block."
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://*.linkedin.com/jobs/*"]
    }
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "icons": {
    "16": "images/application-16.png",
    "32": "images/application-32.png",
    "128": "images/application-128.png"
  },
  "host_permissions": [
    "https://www.linkedin.com/jobs/*",
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*"
  ],
  "permissions": ["scripting", "tabs", "activeTab", "identity"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
